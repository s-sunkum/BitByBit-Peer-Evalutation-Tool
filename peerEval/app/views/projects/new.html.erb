<%# Create project form %>
<h1>Create A Project</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with url: projects_path(:team_id => params[:team_id]) do |f| %>
      <%#= render 'shared/error_messages_project' %>
      <%= f.label :name %>
      <%= f.text_field :name, class: "form-control" %>
      <%= f.submit "Assign Project", class: "btn btn-primary" %>
      <br>
      <br>
      <%= link_to 'Back to Menu', current_teacher, class: "btn btn-default glyphicon glyphicon-menu-left"%>
    <% end %>
  </div>
</div>
