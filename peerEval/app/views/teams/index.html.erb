<h1>Teams</h1>
<div class="btn-group-vertical">
<div class="row">
  <div class="col-md-6 col-md-offset-3">
<% if teacher_logged_in?%>
    <% @teams.each do |t| %>
        <%= link_to t.name, projects_path(:teamID => t.id), :class => "btn btn-primary" %>
    <% end %>
<% else %>
    <% @teams.each do |t| %>
        <%= link_to t.name, projects_path(:teamID => t.id), :class => "btn btn-primary" %>
    <% end %>
<% end %>
</div>
</div>
</div>
